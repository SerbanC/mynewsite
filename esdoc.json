{
  "source": "./src/scripts",
  "destination": "./out/esdoc",
  "access": ["public", "protected", "private"],
  "index": "./README.md",
  "package": "./package.json",
  "title": "serbancarjan.ro Docs",
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^scripts/", "to": ""}
        ]
      }
    }
  ]
}
